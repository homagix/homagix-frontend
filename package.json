{
  "name": "homagix-frontend",
  "version": "5.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "msw": "esbuild --platform=node --bundle src/msw.ts | node",
    "lint": "eslint src/**/*.{ts,vue}",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "vitest --run",
    "test:watch": "vitest",
    "test:coverage": "npm run test:unit -- --coverage",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "update": "npx -y npm-check-updates -d -u -e 2",
    "postinstall": "msw init public --save"
  },
  "engines": {
    "node": ">=20.0.0 <21.0.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "6.5.1",
    "@oruga-ui/oruga-next": "0.8.3",
    "@oruga-ui/theme-bulma": "0.2.11",
    "vue": "3.4.19"
  },
  "devDependencies": {
    "@types/node": "20.11.19",
    "@types/showdown": "2.0.6",
    "@typescript-eslint/eslint-plugin": "7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "@vitejs/plugin-vue": "5.0.4",
    "@vitest/coverage-v8": "^1.2.2",
    "@vue/test-utils": "2.4.4",
    "canvas": "github:jschirrmacher/canvas",
    "esbuild": "0.20.0",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-vue": "9.21.1",
    "happy-dom": "^13.3.8",
    "jwt-decode": "4.0.0",
    "msw": "^2.2.0",
    "prettier": "3.2.5",
    "prettier-eslint": "16.3.0",
    "sass": "1.71.0",
    "typescript": "5.3.3",
    "vite": "5.1.3",
    "vitest": "1.2.2",
    "vue-router": "4.2.5",
    "vue-showdown": "4.2.0",
    "vue-tsc": "1.8.27",
    "vuex": "4.1.0",
    "wordcloud": "1.2.2"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
