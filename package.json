{
  "name": "homagix-frontend",
  "version": "5.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "msw": "esbuild --platform=node --bundle src/msw.ts | node",
    "lint": "eslint src/**/*.{ts,vue}",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "vitest --run",
    "test:watch": "vitest",
    "test:coverage": "npm run test:unit -- --coverage",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "update": "npx -y npm-check-updates -du",
    "postinstall": "msw init public --save"
  },
  "engines": {
    "node": ">=20.0.0 <21.0.0",
    "npm": ">=10.0.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "6.7.2",
    "@oruga-ui/oruga-next": "0.9.3",
    "@oruga-ui/theme-bulma": "0.5.3",
    "vue": "3.5.13"
  },
  "devDependencies": {
    "@types/node": "22.10.7",
    "@types/showdown": "2.0.6",
    "@typescript-eslint/eslint-plugin": "8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "@vitejs/plugin-vue": "5.2.1",
    "@vitest/coverage-v8": "^3.0.2",
    "@vue/test-utils": "2.4.6",
    "canvas": "github:jschirrmacher/canvas",
    "esbuild": "0.24.2",
    "eslint": "8.57.0",
    "eslint-config-prettier": "10.0.1",
    "eslint-plugin-prettier": "5.2.3",
    "eslint-plugin-vue": "9.32.0",
    "graphql": "16.10.0",
    "happy-dom": "^16.6.0",
    "jwt-decode": "4.0.0",
    "msw": "^2.7.0",
    "prettier": "3.4.2",
    "prettier-eslint": "16.3.0",
    "sass": "1.83.4",
    "typescript": "5.7.3",
    "vite": "6.0.7",
    "vitest": "3.0.2",
    "vue-router": "4.5.0",
    "vue-showdown": "4.2.0",
    "vue-tsc": "2.2.0",
    "vuex": "4.1.0",
    "wordcloud": "1.2.3"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
