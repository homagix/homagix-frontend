{
  "name": "homagix-frontend",
  "version": "5.0.0",
  "scripts": {
    "dev": "vite",
    "msw": "esbuild --platform=node --bundle src/msw.ts | node",
    "lint": "eslint src/**/*.{ts,vue}",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "vitest --run",
    "test:watch": "vitest",
    "test:coverage": "npm run test:unit -- --coverage",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "update": "npx -y npm-check-updates -d -u -e 2",
    "postinstall": "msw init public --save"
  },
  "engines": {
    "node": ">=18.0.0 <19.0.0",
    "npm": ">=7.0.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "6.4.2",
    "@oruga-ui/oruga-next": "0.7.0",
    "@oruga-ui/theme-bulma": "0.2.11",
    "vue": "3.3.6"
  },
  "devDependencies": {
    "@types/node": "20.8.7",
    "@types/showdown": "2.0.3",
    "@typescript-eslint/eslint-plugin": "6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "@vitejs/plugin-vue": "4.4.0",
    "@vitest/coverage-v8": "^0.34.6",
    "@vue/test-utils": "2.4.1",
    "canvas": "github:jschirrmacher/canvas",
    "esbuild": "0.19.5",
    "eslint": "8.52.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-prettier": "5.0.1",
    "eslint-plugin-vue": "9.17.0",
    "jsdom": "22.1.0",
    "jwt-decode": "3.1.2",
    "msw": "^1.3.2",
    "prettier": "3.0.3",
    "prettier-eslint": "16.1.1",
    "sass": "1.69.4",
    "typescript": "5.2.2",
    "vite": "4.5.0",
    "vitest": "0.34.6",
    "vue-router": "4.2.5",
    "vue-showdown": "4.2.0",
    "vue-tsc": "1.8.19",
    "vuex": "4.1.0",
    "wordcloud": "1.2.2"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
