{
  "name": "homagix-frontend",
  "version": "5.0.0",
  "scripts": {
    "dev": "vite",
    "msw": "esbuild --platform=node --bundle src/msw.ts | node",
    "lint": "eslint src/**/*.{ts,vue}",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "vitest --run",
    "test:watch": "vitest",
    "test:coverage": "npm run test:unit -- --coverage",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "update": "npx -y npm-check-updates -d -u -e 2",
    "postinstall": "msw init public --save"
  },
  "engines": {
    "node": ">=18.0.0 <19.0.0",
    "npm": ">=7.0.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "6.4.0",
    "@oruga-ui/oruga-next": "0.6.0",
    "@oruga-ui/theme-bulma": "0.2.10",
    "vue": "3.3.4"
  },
  "devDependencies": {
    "@types/node": "20.2.3",
    "@types/showdown": "2.0.1",
    "@typescript-eslint/eslint-plugin": "5.59.7",
    "@vitejs/plugin-vue": "4.2.3",
    "@vitest/coverage-c8": "0.31.1",
    "@vue/test-utils": "2.3.2",
    "c8": "7.13.0",
    "canvas": "github:jschirrmacher/canvas",
    "esbuild": "0.17.19",
    "eslint": "8.41.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-vue": "9.14.0",
    "jsdom": "22.0.0",
    "jwt-decode": "3.1.2",
    "msw": "1.2.1",
    "prettier": "2.8.8",
    "prettier-eslint": "15.0.1",
    "sass": "1.62.1",
    "typescript": "5.0.4",
    "vite": "4.3.8",
    "vitest": "0.31.1",
    "vue-router": "4.2.1",
    "vue-showdown": "4.1.0",
    "vue-tsc": "1.6.5",
    "vuex": "4.1.0",
    "wordcloud": "1.2.2"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
