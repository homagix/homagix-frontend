{
  "name": "homagix-frontend",
  "version": "5.0.0",
  "scripts": {
    "dev": "vite",
    "lint": "eslint src/**/*.ts",
    "test": "vite test",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "postinstall": "msw init public --save"
  },
  "engines": {
    "node": ">=16.0.0 <17.0.0",
    "npm": ">=6.0.0"
  },
  "dependencies": {
    "vue": "3.2.27"
  },
  "devDependencies": {
    "@types/node": "16.11.24",
    "@typescript-eslint/eslint-plugin": "5.10.0",
    "@vitejs/plugin-vue": "2.0.1",
    "esbuild": "0.14.11",
    "eslint": "8.7.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "msw": "0.36.5",
    "prettier": "2.5.1",
    "typescript": "4.5.4",
    "vite": "2.7.13",
    "vue-router": "4.0.12",
    "vue-tsc": "0.30.6",
    "vuex": "4.0.2"
  },
  "jest": {
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    }
  },
  "msw": {
    "workerDirectory": "public"
  }
}
