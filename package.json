{
  "name": "homagix-frontend",
  "version": "5.0.0",
  "scripts": {
    "dev": "vite",
    "msw": "esbuild --platform=node --bundle src/msw.ts | node",
    "lint": "eslint src/**/*.{ts,vue}",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "vitest --run",
    "test:watch": "vitest",
    "test:coverage": "npm run test:unit -- --coverage",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "update": "npx -y npm-check-updates -d -u -e 2",
    "postinstall": "msw init public --save"
  },
  "engines": {
    "node": ">=18.0.0 <19.0.0",
    "npm": ">=7.0.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "6.5.1",
    "@oruga-ui/oruga-next": "0.8.2",
    "@oruga-ui/theme-bulma": "0.2.11",
    "vue": "3.3.13"
  },
  "devDependencies": {
    "@types/node": "20.10.5",
    "@types/showdown": "2.0.6",
    "@typescript-eslint/eslint-plugin": "6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vitejs/plugin-vue": "4.5.2",
    "@vitest/coverage-v8": "^1.1.0",
    "@vue/test-utils": "2.4.3",
    "canvas": "github:jschirrmacher/canvas",
    "esbuild": "0.19.10",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.1",
    "eslint-plugin-vue": "9.19.2",
    "happy-dom": "^12.10.3",
    "jwt-decode": "4.0.0",
    "msw": "^2.0.11",
    "prettier": "3.1.1",
    "prettier-eslint": "16.2.0",
    "sass": "1.69.5",
    "typescript": "5.3.3",
    "vite": "5.0.10",
    "vitest": "1.1.0",
    "vue-router": "4.2.5",
    "vue-showdown": "4.2.0",
    "vue-tsc": "1.8.26",
    "vuex": "4.1.0",
    "wordcloud": "1.2.2"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
