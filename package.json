{
  "name": "homagix-frontend",
  "version": "5.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "msw": "esbuild --platform=node --bundle src/msw.ts | node",
    "lint": "eslint src/**/*.{ts,vue}",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "vitest --run",
    "test:watch": "vitest",
    "test:coverage": "npm run test:unit -- --coverage",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "update": "npx -y npm-check-updates -d -u -e 2",
    "postinstall": "msw init public --save"
  },
  "engines": {
    "node": ">=20.0.0 <21.0.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "6.5.2",
    "@oruga-ui/oruga-next": "0.8.11",
    "@oruga-ui/theme-bulma": "0.3.0",
    "vue": "3.4.30"
  },
  "devDependencies": {
    "@types/node": "20.14.9",
    "@types/showdown": "2.0.6",
    "@typescript-eslint/eslint-plugin": "7.14.1",
    "@typescript-eslint/parser": "^7.14.1",
    "@vitejs/plugin-vue": "5.0.5",
    "@vitest/coverage-v8": "^1.6.0",
    "@vue/test-utils": "2.4.6",
    "canvas": "github:jschirrmacher/canvas",
    "esbuild": "0.21.5",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-vue": "9.26.0",
    "happy-dom": "^14.12.3",
    "jwt-decode": "4.0.0",
    "msw": "^2.3.1",
    "prettier": "3.3.2",
    "prettier-eslint": "16.3.0",
    "sass": "1.77.6",
    "typescript": "5.5.2",
    "vite": "5.3.1",
    "vitest": "1.6.0",
    "vue-router": "4.4.0",
    "vue-showdown": "4.2.0",
    "vue-tsc": "2.0.22",
    "vuex": "4.1.0",
    "wordcloud": "1.2.2"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
